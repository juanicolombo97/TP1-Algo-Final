Object subclass: #AlgoVid	instanceVariableNames: 'registro'	classVariableNames: ''	package: 'tp1-103471'!!AlgoVid methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:37'!nuevaPersona: unaPersona	registro nuevaPersona: unaPersona.! !!AlgoVid methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:37'!initialize	registro :=  RegistroPersonas new.! !!AlgoVid methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 01:29'!agregarSintoma: unSintoma a: unaPersona 	registro agregarSintoma: unSintoma a:  unaPersona.	! !!AlgoVid methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:37'!puedeCircular:  unaPersona 	^  registro puedeCircular: unaPersona.	! !!AlgoVid methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:55'!estaVacunadx: unaPersona 	registro estaVacunadx: unaPersona.	! !!AlgoVid methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 01:35'!diagnosticoDe: unaPersona 	^ (registro diagnosticoDe: unaPersona).	! !Object subclass: #Persona	instanceVariableNames: 'puedeCircular estaVacunado sintomas sintomaComun sintomasComunes'	classVariableNames: ''	package: 'tp1-103471'!!Persona methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:41'!puedeCircular	^ puedeCircular.! !!Persona methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 12:58'!diagnostico		(sintomaComun | (sintomas size = 4)) ifTrue: [ (sintomas size >= 3) ifTrue: [ ^ 'Positivo' ] ifFalse: [ ^ 'Sospechoso']].	^ 'Sospechoso'.! !!Persona methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 12:50'!initialize 		puedeCircular := false.	estaVacunado := false.	sintomasComunes  := OrderedCollection new.	sintomasComunes  add: 'Fiebre'.	sintomasComunes  add: 'Tos'.	sintomasComunes  add: 'Cansancio'.	sintomaComun := false.	sintomas := OrderedCollection new.! !!Persona methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 12:55'!agregarSintoma: unSintoma 	puedeCircular := false.	sintomas add: unSintoma.	(sintomasComunes  includes: unSintoma ) ifTrue: [ sintomaComun := true ].! !!Persona methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:54'!vacunar	puedeCircular := true.	estaVacunado := true.! !Object subclass: #RegistroPersonas	instanceVariableNames: 'registro'	classVariableNames: ''	package: 'tp1-103471'!!RegistroPersonas methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:40'!nuevaPersona: unaPersona 		| personaNueva |		personaNueva := Persona new.	registro at: unaPersona put: personaNueva.! !!RegistroPersonas methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:36'!initialize 	registro := Dictionary new.! !!RegistroPersonas methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 01:32'!agregarSintoma: unSintoma a: unaPersona		(registro at: unaPersona) agregarSintoma: unSintoma.! !!RegistroPersonas methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:41'!puedeCircular: unaPersona 	^ (registro at: unaPersona ) puedeCircular.! !!RegistroPersonas methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 00:54'!estaVacunadx:  unaPersona 			(registro at: unaPersona) vacunar.! !!RegistroPersonas methodsFor: 'initialization' stamp: 'Anonymous 5/20/2021 01:34'!diagnosticoDe: unaPersona 	^(registro at: unaPersona) diagnostico.! !